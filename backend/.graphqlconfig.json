{
  "projects": {
    "app": {
      "schemaPath": "src/schema.graphql",
      "extensions": {
        "endpoints": { "default": "http://localhost:${env:PORT}" }
      }
    },
    "prisma": {
      "schemaPath": "src/generated/prisma.graphql",
      "extensions": { "prisma": "prisma.yml" }
    }
  }
}
